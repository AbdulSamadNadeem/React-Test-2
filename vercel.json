{
    "rewrites": [
      { "source": "/about", "destination": "/about.html" },
      { "source": "/blog/:slug", "destination": "/blog/[slug].html" }
    ],
    "redirects": [
      { "source": "/old-blog", "destination": "/blog", "permanent": true }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ]
  }
  